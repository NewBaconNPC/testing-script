itemesp_tool(v,Color3.fromRGB(164, 66, 245),Color3.fromRGB(255, 255, 255))

end

end)

else

end

end,

})



game:GetService("RunService").Heartbeat:Connect(function()

	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = sp

end)
